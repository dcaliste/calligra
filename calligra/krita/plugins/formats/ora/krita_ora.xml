<?xml version="1.0" encoding="UTF-8"?>
<!--
Copy of the image/openraster, shipped with shared-mime-info > 0.60.
Remove as soon as krita depends on that version.
-->
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="image/openraster">
    <comment>OpenRaster archiving image</comment>
    <magic priority="50">
      <match type="string" value="PK\003\004" offset="0">
        <match type="string" value="mimetype" offset="30">
          <match type="string" value="image/openraster" offset="38"/>
        </match>
      </match>
    </magic>
    <glob pattern="*.ora"/>
  </mime-type>
</mime-info>
